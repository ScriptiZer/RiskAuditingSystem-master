                                                            <td><a class="btn btn-primary" title="Edit Task" asp-action="Edit" asp-controller="Task" asp-route-id="@task.Id"><i class="fa fa-pencil"></i>Edit Task</a></td>

                                                        }
                                                        @if (userPermissions != null && userPermissions.Any(p => p.Permission.Name == "Task" && p.View == true && p.Delete == true))
                                                        {

                                                            <td><button class="btn btn-danger" title="Delete Task" data-actions-id="@task.Id" id="btnDeleteTask"><i class="fa-solid fa-trash"></i>Delete Task</button></td>
                                                        }

                                                    </tr>
