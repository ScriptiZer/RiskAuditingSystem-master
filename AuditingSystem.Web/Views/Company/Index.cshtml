                                                    {
                                                        <tr class="tr8" data-comId="@item.Id">
                                                            <td>
                                                                @(department.Code + " - " + department.Name)
                                                            </td>
                                                            <td>
                                                                @(function.Code + " - " + function.Name)
                                                            </td>
                                                            <td>
                                                                @(activity.Code + " - " + activity.Name)
                                                            </td>

                                                            <td>
                                                                @(objective.Code + " - " + objective.Name)
                                                            </td>
                                                            <td>
                                                                @(task.Code + " - " + task.Name)
                                                            </td>
