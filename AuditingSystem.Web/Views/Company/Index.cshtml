                                {
                                    <tr class="tr3" data-comId="@item.Id">
                                        <th colspan="9">
                                            <a href="/Function/GetFunctionsByDepartmentId?departmentId=@department.Id" title="Department">
                                                @(department.Code + " - " + department.Name)
                                            </a>
                                        </th>

                                        @if (userPermissions != null && userPermissions.Any(p => p.Permission.Name == "Department" && p.View == true && (p.Edit == false && p.Delete == false)))
                                        {

                                            <td><a class="btn btn-primary" title="View Department" asp-action="View" asp-controller="Department" asp-route-id="@department.Id"><i class="fa-solid fa-eye"></i>View Department</a></td>

                                        }
                                        @if (userPermissions != null && userPermissions.Any(p => p.Permission.Name == "Department" && p.View == true && (p.Edit == false && p.Delete == true)))
                                        {

                                            <td> <a class="btn btn-primary" title="View Department" asp-action="View" asp-controller="Department" asp-route-id="@department.Id"><i class="fa-solid fa-eye"></i>View Department</a></td>

                                        }
                                        @if (userPermissions != null && userPermissions.Any(p => p.Permission.Name == "Department" && p.View == true && p.Edit == true))
                                        {

                                            <td> <a class="btn btn-primary" title="Edit Department" asp-action="Edit" asp-controller="Department" asp-route-id="@department.Id"><i class="fa fa-pencil"></i>Edit Department</a></td>

                                        }
                                        @if (userPermissions != null && userPermissions.Any(p => p.Permission.Name == "Department" && p.View == true && p.Delete == true))
                                        {

                                            <td><button class="btn btn-danger" title="Delete Department" data-actions-id="@department.Id" title="Delete" id="btnDeleteDepartment"><i class="fa-solid fa-trash"></i>Delete Department</button></td>
                                        }
                                    </tr>
                                    @foreach (var function in department.Functions.Where(f=>f.IsDeleted == false).OrderBy(f => Convert.ToInt32(f.Code)))
